<?xml version="1.0" encoding="UTF-8"?>
<document xmlns="http://www.skyve.org/xml/document" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="Warehouse" xsi:schemaLocation="http://www.skyve.org/xml/document ../../../schemas/document.xsd">
    <persistent name="WMS_Warehouse"/>
    <singularAlias>Warehouse</singularAlias>
    <pluralAlias>Warehouses</pluralAlias>
    <bizKey expression="Warehouse - {name}"/>
    <attributes>
        <text required="true" name="name">
            <displayName>Name</displayName>
            <length>30</length>
        </text>
        <text required="true" name="code">
            <displayName>Code</displayName>
            <length>20</length>
        </text>
        <integer required="true" name="sort">
            <displayName>Sort</displayName>
        </integer>
    </attributes>
</document>