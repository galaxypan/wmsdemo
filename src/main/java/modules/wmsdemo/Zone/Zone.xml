<?xml version="1.0" encoding="UTF-8"?>
<document xmlns="http://www.skyve.org/xml/document" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="Zone" xsi:schemaLocation="http://www.skyve.org/xml/document ../../../schemas/document.xsd">
    <persistent name="WMS_Zone"/>
    <singularAlias>Zone</singularAlias>
    <pluralAlias>Zones</pluralAlias>
    <bizKey expression="Zone - {name}"/>
    <attributes>
        <text required="true" name="name">
            <displayName>Name</displayName>
            <length>30</length>
        </text>
        <text required="true" name="code">
            <displayName>Code</displayName>
            <length>20</length>
        </text>
    </attributes>
</document>